{
  "name": "@openclaw/wechat",
  "version": "2026.2.1",
  "description": "OpenClaw WeChat channel plugin (json_bot gateway)",
  "type": "module",
  "dependencies": {},
  "devDependencies": {
    "openclaw": "workspace:*"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.26"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "wechat",
      "label": "WeChat",
